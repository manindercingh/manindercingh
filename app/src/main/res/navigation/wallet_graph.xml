<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/wallet_graph"
    app:startDestination="@id/walletHomeFragment">

    <fragment
        android:id="@+id/walletHomeFragment"
        android:name="com.expert.foodbd.wallet.fragments.WalletHomeFragment"
        android:label="fragment_wallet_home"
        tools:layout="@layout/fragment_wallet_home" >
        <action
            android:id="@+id/action_walletHomeFragment_to_activateWalletFragment"
            app:destination="@id/activateWalletFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/activateWalletFragment"
        android:name="com.expert.foodbd.ActivateWalletFragment"
        android:label="fragment_activate_wallet"
        tools:layout="@layout/fragment_activate_wallet" />
</navigation>